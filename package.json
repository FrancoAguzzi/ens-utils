{
  "name": "@namehash/ens-utils",
  "description": "",
  "version": "0.0.0-development",
  "license": "MIT",
  "repository": "namehash/ens-utils",
  "keywords": [
    "ens",
    "ethereum",
    "namehash",
    "ens-utils"
  ],
  "contributors": [
    "Jamie Barton <jamie@notrab.dev>",
    "Franco Aguzzi <frankind.eth@gmail.com>"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --clean=false",
    "test": "vitest",
    "test:watch": "vitest watch",
    "semantic-release": "semantic-release"
  },
  "type": "module",
  "exports": {
    "./currency": {
      "import": "./dist/currency.js",
      "require": "./dist/currency.cjs",
      "types": "./dist/currency.d.ts"
    },
    "./number": {
      "import": "./dist/number.js",
      "require": "./dist/number.cjs",
      "types": "./dist/number.d.ts"
    },
    "./time": {
      "import": "./dist/time.js",
      "require": "./dist/time.cjs",
      "types": "./dist/time.d.ts"
    },
    "./price": {
      "import": "./dist/price.js",
      "require": "./dist/price.cjs",
      "types": "./dist/price.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "date-fns": "3.3.1",
    "decimal.js": "10.4.3"
  },
  "devDependencies": {
    "semantic-release": "23.0.2",
    "tsup": "8.0.2",
    "typescript": "5.3.3",
    "vite": "5.1.4",
    "vitest": "1.3.1"
  },
  "files": [
    "dist"
  ]
}
